script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-51b7f4835a9fac8a"
javascript:
  $(document).ready(function() {
    $(".main").onepage_scroll({
       sectionContainer: "section",
       easing: "ease",
       animationTime: 1000,
       pagination: true,
       updateURL: false
    });
  });
  var isShownArena = false;
  $('.showarena').click(function(){
    $('.arenamore').slideToggle();
    if (isShownArena == false){
      $('.showarena').text("Less");
      isShownArena = true;
    }else{
      $('.showarena').text("More");
      isShownArena = false;
    }
  });
  var isShownCon = false;
  $('.showcon').click(function(){
    $('.conmore').slideToggle();
    if (isShownCon == false){
      $('.showcon').text("Less");
      isShownCon = true;
    }else{
      $('.showcon').text("More");
      isShownCon = false;
    }
  });