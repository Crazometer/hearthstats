<% if @arena.win %>
	var html = '<tr><td><%= image_tag("Icons/Classes/#{@arena.oppclass}_Icon.gif", height: "24", width: "24")%>   <%=@arena.oppclass%></td><td><span class="victory">Victory</span></td></tr>'
	$(html).hide().appendTo('#arenagames > tbody:last').fadeIn(999);
<% else %>
	var html = '<tr><td><%= image_tag("Icons/Classes/#{@arena.oppclass}_Icon.gif", height: "24", width: "24")%>   <%=@arena.oppclass%></td><td><span class="defeat">Defeat</span></td></tr>'
	$(html).hide().appendTo('#arenagames > tbody:last').fadeIn(999);
<% end %>
toastr.success('Arena Game Added');
<% if @runwins > 11 || @runloses > 2 %>
	$('.arenasubmit').fadeOut(function(){
		$('.arenaendrun').fadeIn();
	});
<% end %>