<table class="decklist">
	<tr>
		<th>Name</th>
		<th>Class</th>
		<th>Wins</th>
		<th>Losses</th>
		<th>Win Rate</th>
		<th>Options</th>
	</tr>
	<% @decks.each do |deck| %>
		<tr>
			<td class="name"><%= deck.name %></td>
			<td class="class"><img src="/assets/Icons/Classes/<%= deck.class_name %>_Icon.gif"/></td>
			<td class="wins"><%= deck.wins %></td>
		</tr>
	<% end%>
</table>
