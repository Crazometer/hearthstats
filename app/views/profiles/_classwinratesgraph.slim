div id="#{chartname}"
.cf
javascript:
  var chart = new Highcharts.Chart({
      chart: {
          renderTo: '#{chartname}',
          type: 'column',
          height: '#{height}',
          events: {
            load: function(){
                setTimeout(function() {
                    chart.setSize($("##{chartname}").width(), $("##{chartname}").height());
                }, 1);
            }
        }
      },
      colors: [
        '#f39c12',
        '#ABD473',
        '#69CCF0',
        '#F58CBA',
        '#999999',
        '#f1c40f',
        '#0070DE',
        '#9482C9',
        '#A57A4D'
      ],
      title: {
          text: '#{chartname} Win Rates'
      },
      subtitle: {
          text: 'Source: HearthStats.net'
      },
      xAxis: {
        categories: #{raw railsdata[1]},
          title: {
              text: null
          }
      },
      yAxis: {
          min: 0,
          max: 100,
          title: {
              text: 'Win %',
          },
          labels: {
              overflow: 'justify'
          }
      },
      tooltip: {
          valueSuffix: '%'
      },
      plotOptions: {
          column: {
                colorByPoint: true
              },
          bar: {
              dataLabels: {
                  enabled: true
              }
          },
          series: {
            states: {
              hover: false
            }
          }
      },
      legend: {
        enabled: false
      },
      credits: {
        enabled: false
      },
      series: [{
          name: '#{chartname}',
          data: #{railsdata[0].to_json},
      }]
  });

